  "homepage": "https://github.com/SkyBlockDev/malil-akairo#readme",
  "devDependencies": {
    "@types/centra": "^2.2.0",
    "@types/node": "^14.14.35",
    "@types/node-fetch": "^2.5.8",
    "@typescript-eslint/eslint-plugin": "^4.18.0",
    "@typescript-eslint/parser": "^4.18.0",
