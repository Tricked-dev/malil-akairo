{
	"name": "malil",
	"version": "1.0.0",
	"description": "Malil discord bot",
	"main": "dist/index.js",
	"scripts": {
		"format": "prettier-eslint --write \"{,!(node_modules)/**/}*.{ts,jsx}\"",
		"start": "tsc && node ./dist/index.js",
		"build": "tsc",
		"test": "jest",
		"enmap": "sh ./resources/fixEnmap.sh",
		"dir": "sh resources/mkdir.sh",
		"dev": "nodemon -e ts --exec \"yarn start\"",
		"deb": "nodemon -e ts --exec \"yarn start\"",
		"vps": "npm run build && npm start",
		"lint": "eslint . --ext .ts",
		"lintfix": "eslint . --ext .ts --fix",
		"rm": "rm -r dist/"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/SkyBlockDev/malil-akairo"
	},
	"keywords": [
		"akairo",
		"discordbot",
		"tricked"
	],
	"author": "tricked#3777",
	"license": "UNLICENSED",
	"private": true,
	"bugs": {
		"url": "https://github.com/SkyBlockDev/malil-akairo/issues"
	},
	"homepage": "https://github.com/SkyBlockDev/malil-akairo#readme",
	"devDependencies": {
		"@types/centra": "^2.2.0",
		"@types/node": "^14.14.37",
		"@types/node-fetch": "^2.5.8",
		"@typescript-eslint/eslint-plugin": "^4.18.0",
		"@typescript-eslint/parser": "^4.18.0",
		"babel-eslint": "^10.1.0",
		"discord-rpc": "^3.2.0",
		"eslint": "^7.23.0",
		"eslint-config-prettier": "^8.1.0",
		"eslint-plugin-prettier": "^3.3.1",
		"nodemon": "^2.0.7",
		"prettier": "^2.2.1",
		"prettier-eslint-cli": "^5.0.1",
		"typescript": "^4.2.3"
	},
	"optionalDependencies": {
		"bufferutil": "^4.0.3",
		"utf-8-validate": "^5.0.4",
		"zlib-sync": "^0.1.7"
	},
	"dependencies": {
		"alexa-bot-api": "^0.2.1",
		"asciify-image": "^0.1.6",
		"centra": "^2.4.2",
		"chalk": "^4.1.0",
		"common-tags": "^1.8.0",
		"discord-akairo": "SkyBlockDev/discord-akairo",
		"discord.js": "discordjs/discord.js",
		"enmap": "^5.8.4",
		"express": "^4.17.1",
		"express-rate-limit": "^5.2.6",
		"genius-lyrics-api": "^3.0.6",
		"moment": "^2.29.1",
		"winston": "^3.3.3",
		"winston-daily-rotate-file": "^4.5.1"
	},
	"engines": {
		"node": ">=14.15.1"
	},
	"eslintConfig": {
		"root": true,
		"parser": "@typescript-eslint/parser",
		"plugins": [
			"prettier",
			"@typescript-eslint"
		],
		"extends": [
			"prettier",
			"eslint:recommended",
			"plugin:@typescript-eslint/eslint-recommended",
			"plugin:@typescript-eslint/recommended"
		]
	},
	"eslingignore": [
		"node_modules",
		"dist/*"
	],
	"prettier": {
		"tabWidth": 4,
		"singleQuote": false,
		"semi": true,
		"bracketSpacing": true,
		"arrowParens": "always",
		"printWidth": 200,
		"useTabs": true,
		"endOfLine": "lf"
	}
}
